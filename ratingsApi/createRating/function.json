{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "post"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "type": "cosmosDB",
      "direction": "out",
      "name": "outputDocument",
      "databaseName": "ratingsDB",
      "collectionName": "ratings",
      "createIfNotExists": "true",
      "connectionStringSetting": "srlessratingsdb_DOCUMENTDB"
    }
  ]
}
